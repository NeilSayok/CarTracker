<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="700">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/userNameTV"
            android:id="@+id/floatingActionButton3"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_height="48dp"
            motion:layout_constraintTop_toTopOf="@+id/floatingActionButton3"
            motion:layout_constraintBottom_toBottomOf="@+id/floatingActionButton3"
            android:layout_width="0dp"
            android:id="@+id/userNameET"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/loginTV"
            android:id="@+id/userNameTV"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/loginTV"
            motion:layout_constraintEnd_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="216dp" />
        <Constraint
            android:layout_height="200dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:id="@+id/company_logo3"
            motion:layout_constraintBottom_toTopOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>